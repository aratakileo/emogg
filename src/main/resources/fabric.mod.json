{
  "schemaVersion": 1,
  "id": "emogg",
  "version": "${version}",
  "name": "emogg",
  "description": "Adds emoji support to the chat and to most of the Minecraft interface",
  "authors": [
    "Arataki Leo"
  ],
  "contributors": [
    "Abbie5",
    "Spurkus"
  ],
  "contact": {
    "sources": "https://github.com/aratakileo/emogg",
    "homepage": "https://modrinth.com/mod/emogg",
    "issues": "https://github.com/aratakileo/emogg/issues"
  },
  "license": "GPL-3.0",
  "icon": "assets/emogg/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "io.github.aratakileo.emogg.Emogg"
    ],
    "modmenu": [
      "io.github.aratakileo.emogg.api.ModMenuApiImpl"
    ]
  },
  "mixins": [
    "emogg.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  },
  "accessWidener" : "emogg.accesswidener"
}
